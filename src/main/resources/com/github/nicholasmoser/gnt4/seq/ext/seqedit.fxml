<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.input.KeyCodeCombination?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.nicholasmoser.gnt4.seq.ext.SeqEditor">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New Edit">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="N" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
                  <MenuItem mnemonicParsing="false" text="Open Edit">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="O" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator>
                  </MenuItem>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Save">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="S" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
            <MenuItem mnemonicParsing="false" text="Revert">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="R" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About Seq Editor" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <SplitPane dividerPositions="0.33, 0.666" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <items>
        <AnchorPane>
          <children>
                  <ListView fx:id="editList" prefHeight="751.0" prefWidth="388.0" />
          </children>
        </AnchorPane>
        <AnchorPane>
          <children>
            <Label alignment="CENTER" layoutX="14.0" layoutY="16.0" style="&#10;" text="Name" textAlignment="CENTER" wrapText="false">
                     <font>
                <Font size="18.0" fx:id="x1" />
                     </font></Label>
                  <TextField fx:id="name" layoutX="13.0" layoutY="46.0" prefHeight="25.0" prefWidth="370.0" />
                  <Label alignment="CENTER" layoutX="14.0" layoutY="80.0" style="&#10;" text="Offset" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font size="18.0" fx:id="x12" />
                     </font>
                  </Label>
                  <TextField fx:id="offset" layoutX="13.0" layoutY="109.0" prefHeight="25.0" prefWidth="370.0" />
                  <Label alignment="CENTER" layoutX="14.0" layoutY="147.0" style="&#10;" text="Hijacked Bytes Length" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font size="18.0" fx:id="x1211" />
                     </font>
                  </Label>
                  <TextField fx:id="hijackedBytesLength" layoutX="13.0" layoutY="179.0" prefHeight="25.0" prefWidth="370.0" />
            <Label alignment="CENTER" layoutX="14.0" layoutY="213.0" style="&#10;" text="Hijacked Bytes" textAlignment="CENTER" wrapText="false">
              <font>
                <Font size="18.0" fx:id="x121" />
              </font>
            </Label>
                  <TextArea fx:id="hijackedBytesText" editable="false" layoutX="14.0" layoutY="242.0" prefHeight="127.0" prefWidth="370.0" wrapText="true" />
            <Label alignment="CENTER" layoutX="13.0" layoutY="378.0" style="&#10;" text="New Bytes" textAlignment="CENTER" wrapText="false">
              <font>
                <Font size="18.0" fx:id="x122" />
              </font>
            </Label>
                  <TextArea fx:id="newBytesText" layoutX="13.0" layoutY="407.0" prefHeight="332.0" prefWidth="370.0" wrapText="true" />
          </children>
        </AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="200.0">
               <children>
                  <TextArea fx:id="opcodes" editable="false" layoutX="12.0" layoutY="46.0" prefHeight="696.0" prefWidth="370.0" />
                  <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" minWidth="60.0" prefWidth="-1.0" style="&#10;" text="Opcodes" textAlignment="CENTER" wrapText="false">
                     <font>
                        <Font size="18.0" fx:id="x11" />
                     </font>
                  </Label>
               </children>
            </AnchorPane>
      </items>
    </SplitPane>
    <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
      <children>
        <Label fx:id="leftStatus" maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
          <font>
            <Font size="11.0" fx:id="x3" />
          </font>
          <textFill>
            <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
          </textFill>
        </Label>
        <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
        <Label fx:id="rightStatus" font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
      </children>
      <padding>
        <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
      </padding>
    </HBox>
  </children>
</VBox>
